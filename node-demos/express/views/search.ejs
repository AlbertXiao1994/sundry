<h1>Search Results for <%= search %></h1>

<% if (results.length) { %>
  <ol>
    <% for (var i = 0; i < results.length; i++) { %>
      <li>
        <a href="<%= results[i].url %>"><%= results[i].name %></a> <%= results[i].stargazers_count %>
      </li>
    <% } %>
  </ol>
<% } else { %>
  <p>no result</p>
<% } %>